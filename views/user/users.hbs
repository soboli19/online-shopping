<div class="card">
        <div class="card-header clearfix">
          <h2 class="h3 float-left">Users</h2>
          {{!-- <div class="float-right">
            <a href="#create" class="btn btn-primary">New User</a>
          </div> --}}
        </div>
        <div class="table-responsive">
          <table class="table table-striped table-hover table-bordered">
            <thead>
              <tr>
                <td>Name</td>
                <td>Email</td>
                <td>Created</td>
                <td>Active</td>
                <td>Admin</td>
              </tr>
            </thead>
            <tbody>
                {{#each users}}
                <tr>
                    <td>{{ this.name }}</td>
                    <td><a href="/edit-user/{{ this._id }}">{{ this.email}}</a> </td>
                    <td>{{ this.created }}</td>
                    <td>{{ this.active }}</td>
                    <td>{{ this.admin }}</td>
                </tr>
                {{/each}}   
            </tbody>
        </table>
    </div>
</div